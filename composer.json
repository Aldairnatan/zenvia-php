{
    "name": "artesaos/zenvia-php",
    "description": "Zenvia API 2.0 with your PHP application, build on top of the psr-7 components.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Mauri de Souza Nunes",
            "email": "mauri870@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Artesaos\\Zenvia\\": "src/Artesaos/Zenvia"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artesaos\\Zenvia\\Tests\\": "tests/Zenvia"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1 | ~5.2",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "php-http/guzzle6-adapter": "~1.0",
        "php-http/message": "^1.1",
        "puli/composer-plugin": "1.0.0-beta9"
    }
}
